<div class="row sign-up-form">
    <div class="col s12 m6">
        <div class="card blue-grey darken-1">
            <div class="card-content white-text">
                <span class="card-title">Course Code: <%= @course.short_name %></span>
                <p>Name: <%= @course.name %></p>
                <p>Description: <%= @course.description %></p>
            </div>            
        </div>
        <%= link_to  '<i class="material-icons left">create</i>Edit your profile'.html_safe, 
            edit_course_path(@course), class: "waves-effect waves-light btn"  %>
        
    </div>
</div>